<Page x:Class="Sablona.Prihlaseni"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Sablona"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="Prihlaseni">

    <Grid>
        <TextBox x:Name="log" Height="20" Width="80" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,20"/>
        <TextBox x:Name="pass" Height="20" Width="80" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="110,155,110,125"/>
        <Button x:Name="conf" Height="20" Width="59" Content="Přihlásit" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="120,180,121,100" Click="conf_Click"/>
    </Grid>
</Page>
